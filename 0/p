FoamFile
{
    version     7.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    xNeg
    {
        type            zeroGradient;
    }
    xPos
    {
        type            zeroGradient;
    }
    yPos
    {
        type            zeroGradient;
    }
    yNeg
    {
        type            zeroGradient;
    }
    zPlan
    {
        type            empty;
    }
}


